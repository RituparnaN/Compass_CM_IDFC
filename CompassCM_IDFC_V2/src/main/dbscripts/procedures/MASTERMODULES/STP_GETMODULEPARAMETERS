CREATE OR REPLACE PROCEDURE STP_GETMODULEPARAMETERS (
i_vcModuleType IN VARCHAR2, i_vcUserCode  IN VARCHAR2, 
i_vcUserRole   IN VARCHAR2, i_vcIpAddress IN VARCHAR2, 
i_vcResultSet  IN OUT SYS_REFCURSOR)
AS
BEGIN
	OPEN i_vcResultSet FOR
  SELECT MODULECODE, MODULENAME, MODULEPARAMNAME, MODULEPARAMALIASNAME, MODULEPARAMIDNAME,
  		 MODULEPARAMINDEX, MODULEPARAMDATATYPE, NVL(TRIM(MODULEPARAMVIEWNAME), 'NA') MODULEPARAMVIEWNAME,
		 MODULEPARAMSTATICVALUES, MODULEPARAMDEFAULTVALUE,
		 MODULEPARAMVALIDATIONFIELD, MODULEPARAMVALIDATIONTYPE
    FROM TB_MODULEPARAMETERS
   WHERE MODULECODE =  i_vcModuleType
   ORDER BY MODULEPARAMINDEX ASC;
END STP_GETMODULEPARAMETERS;
/
 
