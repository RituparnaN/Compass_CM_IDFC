CREATE TABLE TB_CHATMESSAGES(
MESSAGESEQNO			NUMBER(11),
CHATWINDOWID			VARCHAR2(400),
MESSAGEID				VARCHAR2(400),
FROMUSER				VARCHAR2(200),
TOUSER					VARCHAR2(200),
MESSAGECONTENT			VARCHAR2(4000),
MESSAGETIMESTAMP		TIMESTAMP,
SEENFLAG				VARCHAR2(1)
);


CREATE SEQUENCE CHATMESSAGESEQUENCE
START WITH 1
INCREMENT BY 1;
